<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="edu.iu.nwb.core"
   name="NWB Core Plug-in"
   version="1.0.0"
   provider-name="Team IVC"
   class="edu.iu.nwb.common.persisters.PersisterProviderPlugin">

   <runtime>
      <library name="NWB.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="edu.iu.iv.core"/>
   </requires>

   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="edu.iu.nwb.common.persisters.PersisterProviderPlugin">
      </startup>
   </extension>

</plugin>
