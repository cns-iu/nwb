<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="edu.iu.informatics.analysis.pagerank"
   name="PageRankAlgorithm"
   version="1.0.0"
   provider-name="Santo">

   <runtime>
      <library name="PageRankAlgorithm.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="edu.iu.iv.core"/>
      <import plugin="edu.iu.informatics.shared"/>
      <import plugin="edu.iu.nwb.core"/>
   </requires>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="edu.iu.informatics.analysis.pagerank.actionSet"
            visible="true"
            id="edu.iu.informatics.analysis.pagerank.actionSet">
         <action
               label="PageRankAlgorithm"
               class="edu.iu.informatics.analysis.pagerank.PageRankAlgorithmPlugin"
               tooltip="Select to launch PageRankAlgorithm"
               menubarPath="Analysis/additions"
               id="edu.iu.informatics.analysis.pagerank.PageRankAlgorithmPlugin">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="edu.iu.informatics.analysis.pagerank.PageRankAlgorithmPlugin">
      </startup>
   </extension>

</plugin>
