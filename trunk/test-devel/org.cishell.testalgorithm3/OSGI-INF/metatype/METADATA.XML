		 	<Option label="Verbose" value="Verbose"/>
		 </AD>
		 
		 <AD name="Save log files to" id="logFileSaveDir" type="String" default="directory:/tmp/"/>
	</OCD>
	
	<Designate pid="org.cishell.testalgorithm3.TestAlgorithm">
