<?xml version="1.0" encoding="UTF-8"?>
<metatype:MetaData xmlns:metatype="http://www.osgi.org/xmlns/metatype/v1.0.0">
	<OCD name="Barabasi-Albert" id="edu.iu.nwb.modeling.barabasialbert.gui"
		 description="Barabasi-Albert">
		<AD name="Number of nodes" id="NumNodes" 
			type="Integer" default="1000" min="0"
                        description="Number of nodes of the network"/> 
<!-- Put extremes of range, if unknown leave blank -->
<!-- Can be required from users for information -->
		<AD name="Links set by new node" id="NewLinks" 
			type="Integer" default="1" min="1"                        
                        description="Number of links set by new node, must be at least 1"/> 
		<AD name="Number of bins" id="NumBins" 
			type="Integer" default="10" min="8"
                        description="Number of bins for the degree distribution, must be at least 8"/>
	</OCD>
	<Designate pid="edu.iu.nwb.modeling.barabasialbert">
		<Object ocdref="edu.iu.nwb.modeling.barabasialbert.gui" />
	</Designate>
</metatype:MetaData>
