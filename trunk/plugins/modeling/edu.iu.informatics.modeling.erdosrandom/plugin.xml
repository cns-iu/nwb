<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="edu.iu.informatics.modeling.erdosrandom"
   name="RandomGraph Plug-in"
   version="1.0.0"
   provider-name="Team IVC">

   <runtime>
      <library name="randomGraph.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="edu.iu.iv.core"/>
      <import plugin="edu.iu.informatics.shared"/>
   </requires>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="edu.iu.informatics.randomGraph.actionSet"
            visible="true"
            id="edu.iu.informatics.randomGraph.actionSet">
         <action
               label="Erdos Random Graph"
               class="edu.iu.informatics.modeling.erdosrandom.ErdosRandomPlugin"
               tooltip="Select to launch &apos;Random Graph&apos;"
               menubarPath="Modeling/additions"
               id="edu.iu.informatics.modeling.erdosrandom.ErdosRandomPlugin">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="edu.iu.informatics.modeling.erdosrandom.ErdosRandomPlugin">
      </startup>
   </extension>

</plugin>
