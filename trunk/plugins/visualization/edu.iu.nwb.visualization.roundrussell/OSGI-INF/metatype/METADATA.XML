<?xml version="1.0" encoding="UTF-8"?>
<metatype:MetaData xmlns:metatype="http://www.osgi.org/xmlns/metatype/v1.0.0">
	<OCD name="Round Russell" id="edu.iu.nwb.visualization.roundrussell.RoundRussellAlgorithm.OCD"
		 	description="Provides Circular (Round Russell) Visualization on the netwrok provided via nwb file.">
		<AD name="Degree of Edge Bundling" id="betacolumn" type="Float" description="Degree of edge bundling must be between 0.0 and 1.0 inclusive." default="0.75"/> 	
		<AD name="Node Strength Column" id="strengthcolumn" type="String" description="Column with node strength value." default="strength"/>
		<AD name="Level 1 Column" id="level1_column" type="String" description="Column with level 1 information." default="level_1"/>
		<AD name="Level 2 Column" id="level2_column" type="String" description="Column with level 2 information." default="level_2"/>
		<AD name="Level 3 Column" id="level3_column" type="String" description="Column with level 3 information." default="level_3"/>
		<AD name="Level 4 Column" id="level4_column" type="String" description="Column with level 4 information." default="level_4"/>
		<AD name="Edge Weight Column" id="weightcolumn" type="String" description="Column with edge weight value." default="weight"/>	
	</OCD>
	<Designate pid="edu.iu.nwb.visualization.roundrussell.RoundRussellAlgorithm">
		<Object ocdref="edu.iu.nwb.visualization.roundrussell.RoundRussellAlgorithm.OCD" />
	</Designate>
</metatype:MetaData>
