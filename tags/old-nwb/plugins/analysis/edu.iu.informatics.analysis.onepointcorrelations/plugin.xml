<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="edu.iu.informatics.analysis.onepointcorrelations"
   name="OnePointCorrelations Plug-in"
   version="1.0.0"
   provider-name="Team IVC">

   <runtime>
      <library name="onePointCorrelations.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="edu.iu.iv.core"/>
      <import plugin="edu.iu.informatics.shared"/>
      <import plugin="edu.iu.nwb.core"/>
   </requires>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="edu.iu.informatics.analysis.onepointcorrelations.actionSet"
            visible="true"
            id="edu.iu.informatics.analysis.onepointcorrelations.actionSet">
         <action
               label="One Point Correlations"
               class="edu.iu.informatics.analysis.onepointcorrelations.OnePointCorrelationPlugin"
               tooltip="Select to launch OnePointCorrelations Plug-in"
               menubarPath="Analysis/additions"
               id="edu.iu.informatics.analysis.onepointcorrelations.OnePointCorrelationPlugin">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="edu.iu.informatics.analysis.onepointcorrelations.OnePointCorrelationPlugin">
      </startup>
   </extension>

</plugin>
